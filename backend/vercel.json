{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/workouts/(.*)",
        "dest": "/api/workouts"
      },
      {
        "src": "/api/user/(.*)",
        "dest": "/api/user"
      }
    ]
}